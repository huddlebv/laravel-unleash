{
    "name": "huddlebv/laravel-unleash",
    "description": "An Unleash client for Laravel",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "unleash/client": "^v2",
        "symfony/cache": "^6.2|^7.2",
        "psr/cache": "^1.0|^2.0|^3.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "JWebb\\Unleash\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jonathan Webb",
            "email": "jonwebb247@gmail.com"
        },
        {
            "name": "Wessel Verheij",
            "email": "wessel@plugandpay.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "JWebb\\Unleash\\ServiceProvider"
            ],
            "aliases": {
                "Unleash": "JWebb\\Unleash\\Facades\\Unleash"
            }
        }
    }
}
